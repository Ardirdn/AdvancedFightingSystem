{
  "name": "AdvancedFightingSystem",
  "tree": {
    "$className": "DataModel",
    "ReplicatedStorage": {
      "Modules": {
        "$path": "src/shared"
      }
    },
    "ServerScriptService": {
      "$path": "src/server"
    },
    "StarterPlayer": {
      "StarterPlayerScripts": {
        "$path": "src/client"
      }
    },
    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      },
      "FightingArena": {
        "$className": "Folder"
      },
      "Leaderboard": {
        "$className": "Folder"
      }
    },
    "Lighting": {
      "$properties": {
        "Ambient": [
          0.2,
          0.2,
          0.25
        ],
        "Brightness": 2,
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "Future"
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}